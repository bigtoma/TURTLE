<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemView">
        <attr name="left_icon" format="integer"/>
        <attr name="show_left_icon" format="boolean"/>
        <attr name="left_text" format="string"/>
        <attr name="right_text" format="string"/>
        <attr name="right_arrow" format="integer"/>
        <attr name="show_right_arrow" format="boolean"/>
        <attr name="show_bottom_line" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="PullScrollView">
        <attr name="header" format="reference" />
        <attr name="headerHeight" format="dimension" />
        <attr name="headerTopHeight" format="dimension"/>
        <attr name="headerVisibleHeight" format="dimension" />
    </declare-styleable>
</resources>